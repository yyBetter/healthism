.float-button {
  position: fixed;
  right: 30rpx;
  bottom: 200rpx;
  width: 120rpx;
  height: 120rpx;
  background: #07c160;
  border-radius: 50%;
  box-shadow: 0 6rpx 16rpx rgba(0, 0, 0, 0.3);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9999;
}

.button-inner {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #fff;
}

.iconfont {
  font-size: 50rpx;
  margin-bottom: 6rpx;
}

.text {
  font-size: 26rpx;
} 