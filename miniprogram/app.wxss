@font-face {
  font-family: "iconfont";
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-left:before {
  content: "\e6ef";
}

.icon-right:before {
  content: "\e6f0";
}

.icon-calendar:before {
  content: "\e6f1";
} 